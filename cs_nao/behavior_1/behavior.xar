                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import socket

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def onInput_onStart(self):
        memProxy = ALProxy("ALMemory")

        HOST = memProxy.getData("udp_address")
        PORT = memProxy.getData("udp_port")
        MESSAGE = memProxy.getData("udp_input")

        self.socket = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
        self.logger.info("Sending data : {}".format(MESSAGE))
        self.socket.connect((HOST, PORT))
        self.socket.send(MESSAGE)
        self.socket.close()
        self.logger.info("Stopping connection")
        self.onStopped()

    def onInput_onStop(self):
        self.onUnload()
        self.onStopped()]]>
</content>
